<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<CloudService id="CloudService">
	<SYBLDirective
		Constraints="Co4:CONSTRAINT cost.PerHour &lt; 60 Euro; Co5:CONSTRAINT cost.PerClientPerHour &lt; 3 Euro" />
	<ServiceTopology id="MainTopology">
		<ServiceTopology id="WebServiceTopology">
			<SYBLDirective Constraints="Co3:CONSTRAINT responseTime &lt; 450 ms" />
			<Relationship>
				<Master>LoadBalancer</Master>
				<Slave>WebService</Slave>
			</Relationship>
			<ServiceUnit defaultImage="6ed288e8-3da1-48f8-abd2-1d45ee7e77a5"
				id="LoadBalancer" />
			<ServiceUnit defaultImage="" associatedOpenstackSnapshot="hadoopSlave"
				id="WebService">
				<SYBLDirective
					Strategies="St1:STRATEGY CASE responseTime &lt; 360 ms AND throughput_average &lt; 300 : scalein" />
			</ServiceUnit>
		</ServiceTopology>
		<ServiceTopology id="DataServiceTopology">
			<SYBLDirective
				Constraints="Co1:CONSTRAINT latency.average &lt; 30 ms; Co2:CONSTRAINT cpu.usage &lt; 80 %" />
			<ServiceTopology id="DataServiceTopology">
				<Relationship>
					<Master>CassandraController</Master>
					<Slave>CassandraNode</Slave>
				</Relationship>
				<ServiceUnit defaultImage="74c744c9-e981-44ce-a8d3-df12028aff37"
					defaultFlavor="2" id="CassandraController" />
				<ServiceUnit defaultImage="be6b1aa4-2227-4f4d-9c63-7aabc9cc3a85"
					defaultFlavor="2" id="CassandraNode" />
			</ServiceTopology>
		</ServiceTopology>
	</ServiceTopology>
</CloudService>